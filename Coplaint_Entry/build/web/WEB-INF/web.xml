<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Complaint_entry</servlet-name>
        <servlet-class>main.Complaint_entry</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Complaint_entry_save</servlet-name>
        <servlet-class>main.Complaint_entry_save</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Complaint_entry_Register</servlet-name>
        <servlet-class>main.Complaint_entry_Register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Download</servlet-name>
        <servlet-class>main.Download</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Complaint_entry</servlet-name>
        <url-pattern>/Complaint_entry</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Complaint_entry_save</servlet-name>
        <url-pattern>/Complaint_entry_save</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Complaint_entry_Register</servlet-name>
        <url-pattern>/Complaint_entry_Register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Download</servlet-name>
        <url-pattern>/Download</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
