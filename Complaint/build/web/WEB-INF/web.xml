<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Complaint</servlet-name>
        <servlet-class>main.Complaint</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Complaint_Register</servlet-name>
        <servlet-class>main.Complaint_Register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>c</servlet-name>
        <servlet-class>main.c</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>alter</servlet-name>
        <servlet-class>main.alter</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>alter_register</servlet-name>
        <servlet-class>main.alter_register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>open_alter_screen</servlet-name>
        <servlet-class>main.open_alter_screen</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Complaint</servlet-name>
        <url-pattern>/Complaint</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Complaint_Register</servlet-name>
        <url-pattern>/Complaint_Register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>c</servlet-name>
        <url-pattern>/c</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>alter</servlet-name>
        <url-pattern>/alter</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>alter_register</servlet-name>
        <url-pattern>/alter_register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>open_alter_screen</servlet-name>
        <url-pattern>/open_alter_screen</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
